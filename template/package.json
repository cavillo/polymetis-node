    "@types/mocha": "5.2.6",
    "mocha": "^6.2.3",
    "ts-node": "^7.0.1",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "dotenv": "^6.1.0",
    "lodash": "^4.17.13",
    "polymetis-node": "^2.0.7"
  }
